page.includeCSS.project = {$plugin.tx_project.settings.scss} 



plugin.tx_project_pi1 {
    view {
        templateRootPaths.0 = EXT:project/Resources/Private/Templates/
        templateRootPaths.1 = {$plugin.tx_project.view.templateRootPath}
        partialRootPaths.0 = EXT:project/Resources/Private/Partials/
        partialRootPaths.1 = {$plugin.tx_project.view.partialRootPath}
        layoutRootPaths.0 = EXT:project/Resources/Private/Layouts/
        layoutRootPaths.1 = {$plugin.tx_project.view.layoutRootPath}
    }
    persistence {
        storagePid = {$plugin.tx_project.persistence.storagePid}
        #recursive = 1
        classes {
      		Hda\Project\Domain\Model\Lecturers {
        		mapping {
	          		tableName = fe_users
	            	recordType = 0
          		}
        	}
      	}        
    }
    features {
        #skipDefaultArguments = 1
        # if set to 1, the enable fields are ignored in BE context
        ignoreAllEnableFieldsInBe = 0
        # Should be on by default, but can be disabled if all action in the plugin are uncached
        requireCHashArgumentForActionArguments = 1
    }


}
